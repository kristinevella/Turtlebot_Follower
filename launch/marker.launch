<launch>
    <arg name="x"/>
    <arg name="y"/>
    <arg name="z"/>
    <arg name="roll"/>
    <arg name="pitch"/>
    <arg name="yaw"/>

    <arg name="urdf_robot_file"/>
    <arg name="robot_name"/>

    <param name="robot_description" command="cat '$(find turtlebot_follower)/urdf/tag36_11_00000.urdf'"/>
    <param name="tf_prefix" value="marker_tf" />

    <node name="spawn_marker" pkg="gazebo_ros" type="spawn_model"
     args="-urdf -x $(arg x) -y $(arg y) -z $(arg z) -R $(arg roll) -P $(arg pitch) -Y $(arg yaw) -model $(arg robot_name) -param robot_description"
     respawn="false" output="screen" />

</launch>